{
  "extends": "./node_modules/gts/",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "^_"
      }
    ],
    "node/no-extraneous-import": [
      "error",
      {
        "allowModules": ["colors"]
      }
    ]
  }
}
