{
  "name": "@amagaki/amagaki",
  "bin": {
    "amagaki": "./src/index.js"
  },
  "version": "0.1.5",
  "license": "MIT",
  "homepage": "https://amagaki.dev",
  "repository": {
    "type": "git",
    "url": "git://github.com/blinkkcode/amagaki.git"
  },
  "engines": {
    "node": ">=11.14.0"
  },
  "scripts": {
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "copy": "mkdir -p dist/src/static; cp src/static/* dist/src/static",
    "dev:start": "npm run copy; npm run compile",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "docs": "typedoc --out docs src/",
    "fix": "node ./node_modules/eslint/bin/eslint --fix src/**/*.ts",
    "prepare": "rm -rf dist/; npm run dev:start;",
    "test": "ava"
  },
  "ava": {
    "require": [
      "ts-node/register"
    ],
    "extensions": [
      "ts"
    ],
    "files": [
      "src/*.test.ts"
    ]
  },
  "devDependencies": {
    "@types/async": "^3.2.5",
    "@types/cli-progress": "^3.8.0",
    "@types/cli-table": "^0.3.0",
    "@types/express": "^4.17.9",
    "@types/js-yaml": "^3.12.5",
    "@types/marked": "^1.2.1",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^13.11.1",
    "@types/nunjucks": "^3.1.3",
    "@types/semver": "^7.3.4",
    "ava": "^3.14.0",
    "eslint": "^7.15.0",
    "eslint-plugin-sort-class-members": "^1.9.0",
    "gts": "^2.0.2",
    "nodemon": "^2.0.4",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typedoc": "^0.19.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "async": "^3.2.0",
    "cli-progress": "^3.8.2",
    "cli-table": "^0.3.4",
    "commander": "^5.1.0",
    "express": "^4.17.1",
    "js-yaml": "^3.14.1",
    "marked": "^1.2.6",
    "mime-types": "^2.1.27",
    "nunjucks": "^3.2.2"
  }
}
