{
  "name": "@amagaki/amagaki-engine-preact",
  "version": "1.1.0",
  "license": "MIT",
  "homepage": "https://amagaki.dev",
  "repository": {
    "type": "git",
    "url": "git://github.com/blinkk/amagaki.git",
    "directory": "packages/amagaki-engine-preact"
  },
  "engines": {
    "node": ">=14"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build:example": "amagaki build example",
    "check": "gts check",
    "clean": "gts clean",
    "dev": "nodemon --watch src -e ts --exec npm tsc",
    "fix": "node ./node_modules/eslint/bin/eslint --fix src/**/*.ts",
    "prepare": "rm -rf dist/; tsc; rm dist/*.tsbuildinfo",
    "test": "ava"
  },
  "ava": {
    "require": [
      "ts-node/register"
    ],
    "extensions": [
      "ts",
      "tsx"
    ],
    "files": [
      "src/*.test.ts",
      "src/*.test.tsx"
    ],
    "timeout": "20s"
  },
  "devDependencies": {
    "@amagaki/amagaki-plugin-page-builder": "^3.3.0",
    "@types/jsdom": "^16.2.14",
    "ava": "^4.0.1",
    "eslint": "^7.15.0",
    "eslint-plugin-sort-class-members": "^1.9.0",
    "gts": "^3.1.0",
    "jsdom": "^16.7.0",
    "nodemon": "^2.0.4",
    "ts-loader": "^7.0.5",
    "ts-node": "^10.5.0",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "@amagaki/amagaki": "*",
    "esbuild-register": "^3.3.2",
    "preact": "^10.6.6",
    "preact-render-to-string": "^5.1.19"
  },
  "files": [
    "dist/"
  ]
}
