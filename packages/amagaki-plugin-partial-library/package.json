{
  "name": "@amagaki/amagaki-plugin-partial-library",
  "license": "MIT",
  "homepage": "https://amagaki.dev",
  "repository": {
    "type": "git",
    "url": "git://github.com/blinkk/amagaki.git",
    "directory": "packages/amagaki-plugin-partial-library"
  },
  "engines": {
    "node": ">=14"
  },
  "version": "0.0.0",
  "main": "./dist/index.js",
  "scripts": {
    "build:example": "npx @amagaki/amagaki build example",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc; npx gulp build;",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "dev:start": "npm run compile",
    "fix": "eslint --fix src/**/*.ts",
    "prepare": "rm -rf dist/; npm run compile",
    "start:example": "npx @amagaki/amagaki serve example",
    "test": "npm run compile; ava"
  },
  "ava": {
    "require": [
      "ts-node/register"
    ],
    "extensions": [
      "ts"
    ],
    "files": [
      "src/**/*.test.ts"
    ],
    "timeout": "20s"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.9",
    "@types/js-beautify": "^1.13.3",
    "@types/node": "^17.0.44",
    "@types/nunjucks": "^3.2.0",
    "ava": "^4.3.0",
    "child_process": "^1.0.2",
    "esbuild": "^0.14.23",
    "eslint": "^8.17.0",
    "eslint-plugin-sort-class-members": "^1.14.1",
    "gts": "^3.1.0",
    "gulp": "^4.0.2",
    "nodemon": "^2.0.4",
    "nunjucks": "^3.2.3",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@amagaki/amagaki": "*"
  },
  "files": [
    "dist/",
    "!**/*.test.*"
  ]
}
