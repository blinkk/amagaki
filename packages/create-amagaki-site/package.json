{
  "name": "create-amagaki-site",
  "version": "0.0.1",
  "keywords": [
    "amagaki"
  ],
  "description": "Create Amagaki sites with one command",
  "repository": {
    "type": "git",
    "url": "https://github.com/blinkk/amagaki",
    "directory": "packages/create-amagaki-site"
  },
  "license": "MIT",
  "bin": {
    "create-amagaki-site": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "ncc build ./src/index.ts -w -o dist/",
    "prerelease": "rimraf ./dist/",
    "release": "ncc build ./src/index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "prepublish": "npm run release"
  },
  "devDependencies": {
    "@types/async-retry": "1.4.2",
    "@types/node": "^12.6.8",
    "@types/parse-github-repo-url": "^1.4.0",
    "@types/prompts": "2.0.1",
    "@types/rimraf": "3.0.0",
    "@types/tar": "4.0.3",
    "@types/validate-npm-package-name": "3.0.0",
    "@vercel/ncc": "0.33.1",
    "async-retry": "1.3.1",
    "chalk": "2.4.2",
    "commander": "2.20.0",
    "got": "10.7.0",
    "parse-github-repo-url": "^1.4.1",
    "prompts": "2.1.0",
    "rimraf": "^3.0.0",
    "tar": "4.4.10",
    "validate-npm-package-name": "3.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
