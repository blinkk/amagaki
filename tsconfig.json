{
    "extends": "./node_modules/gts/tsconfig-google.json",
    "compilerOptions": {
      "esModuleInterop": true,
      "rootDir": ".",
      "outDir": "dist",
      "resolveJsonModule": true,
      "types": [
        "node"
      ]
    },
    "include": [
      "src/**/*.ts",
      "test/**/*.ts"
    ],
    "typedocOptions": {
      "excludeProtected": true,
      "excludeExternals": true,
      "entryPoints": [
        "./src/**/*.ts"
      ],
      "exclude": [
        "./src/*.test.ts",
      ],
    }
  }
