{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "module": "ES2020",
    "esModuleInterop": true,
    "rootDir": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "types": [
      "node",
    ]
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts",
  ],
  "typedocOptions": {
    "excludeProtected": true,
    "excludeExternals": true,
    "entryPoints": [
      "./src/",
    ],
    "exclude": [
      "./src/**/*.test.ts",
    ],
    "out": "docs",
  }
}
