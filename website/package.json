{
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/blinkk/amagaki.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "browsersync:reload": "browser-sync reload",
    "browsersync:start": "browser-sync start --proxy localhost:8080 --no-open --files dist",
    "build": "npm run build:src; node ../dist/src/cli.js build",
    "build:src": "npm run gulp:build;",
    "dev": "npm-run-all --parallel watch:pages start gulp:watch browsersync:start",
    "fix": "npm run fix:sass",
    "fix:sass": "sass-lint-auto-fix -c .sasslintrc.fix.yaml",
    "gulp:watch": "gulp watch",
    "gulp:build": "gulp build",
    "start": "node ../dist/src/cli.js serve",
    "watch:src": "nodemon --watch src -e ts,js,sass --exec npm run build:src",
    "watch:pages": "nodemon --watch content --watch views -e njk,yaml --exec npm run browsersync:reload"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.9",
    "@types/gulp-autoprefixer": "0.0.33",
    "@types/gulp-dart-sass": "^1.0.1"
  },
  "dependencies": {
    "@amagaki/amagaki": "file:../",
    "@amagaki/amagaki-plugin-page-builder": "1.4.1",
    "browser-sync": "^2.27.7",
    "common-tags": "^1.8.0",
    "esbuild": "^0.13.15",
    "github-markdown-css": "^4.0.0",
    "glob": "^7.1.6",
    "gts": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-dart-sass": "^1.0.2",
    "highlight.js": "^10.7.2",
    "jsdom": "^16.5.3",
    "marked": "^2.0.6",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "sucrase": "^3.20.3",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2",
    "uuid": "^8.3.2"
  }
}
